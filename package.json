{
  "name": "display-mime-react",
  "version": "0.0.0-development",
  "description": "Display a resource in React using its MIME data",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges",
    "test": "jest",
    "lint": "eslint lib/* __tests__/* --ext .ts",
    "lint:fix": "eslint lib/* __tests__/* --ext .ts --fix",
    "build": "tsc",
    "prepare": "tsc",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeswr/display-mime-react.git"
  },
  "keywords": [
    "MIME",
    "React",
    "Display"
  ],
  "author": "Jesse Wright <jesse.wright@anu.edu.au>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeswr/display-mime-react/issues"
  },
  "homepage": "https://github.com/jeswr/display-mime-react#readme.md",
  "devDependencies": {
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.19",
    "@types/jest-axe": "^3.5.1",
    "@types/mime": "^2.0.3",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-extra-rules": "*",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unused-imports": "^1.1.1",
    "jest": "^26.6.3",
    "jest-axe": "^4.1.0",
    "pre-commit": "^1.2.2",
    "react": "^17.0.2",
    "semantic-release": "^17.3.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "pre-commit": [
    "lint",
    "build",
    "test"
  ],
  "release": {
    "branches": [
      "main",
      "+([0-9])?(.{+([0-9]),x}).x",
      "next",
      {
        "name": "alpha",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@jeswr/use-state": "^1.0.3",
    "mime": "^2.5.2"
  }
}
